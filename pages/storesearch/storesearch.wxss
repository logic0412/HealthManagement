.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
  padding: 20px;
}

.maMap {
  margin-top: 20px;
  border-radius: 10px; 
  box-shadow: 0 4px 6px rgba(0,0,0,0.1); 
}

.search-button button {
  border: 1px solid #1AAD19;
  background-color: #1AAD19;
  color: white;
  border-radius: 5px; 
  cursor: pointer;
  display: block; 
  margin: 10px 0; 
}
